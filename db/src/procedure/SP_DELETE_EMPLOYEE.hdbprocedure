PROCEDURE "HANA_TRAINING.procedure::SP_DELETE_EMPLOYEE"(
   IN ip_row TABLE (SAPNO INTEGER),
   OUT op_message NVARCHAR(1000)
 )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER AS
   --DEFAULT SCHEMA <default_schema_name>
   -- READS SQL DATA AS
BEGIN
   MERGE INTO "HANA_TRAINING.data::EMPLOYEE" t1 USING :ip_row t2 ON t1.SAPNO = t2.SAPNO
   WHEN MATCHED THEN UPDATE SET t1.DELETEFLAG = 1;

   op_message = 'Data successfully updated!';
END