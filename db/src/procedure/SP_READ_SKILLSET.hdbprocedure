PROCEDURE "HANA_TRAINING.procedure::SP_READ_SKILLSET"(
   IN ip_sapno INTEGER,
   OUT op_table TABLE (
        SAPNO INTEGER,
        SKILLID INTEGER,
        PROFLEVEL NVARCHAR(2),
        DATESTART NVARCHAR(500),
        DATEEND NVARCHAR(500),
        DELETEFLAG NVARCHAR(1)
   )
 )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER AS
   --DEFAULT SCHEMA <default_schema_name>
   -- READS SQL DATA AS
BEGIN
   op_table = SELECT * FROM "HANA_TRAINING.data::SKILLSET" WHERE SAPNO = :ip_sapno;
END